    "-monitor unix:/tmp/<-(socket)->,server,nowait",
    "-chardev socket,id=chrtpm,path=/tmp/<-(tpm)-> ",
    "-tpmdev emulator,id=tpm0,chardev=chrtpm",
    "-device tpm-tis,tpmdev=tpm0",
    "-drive file=<-(isowin)->,media=cdrom",
    "-drive file=drives/virtio-win.iso,media=cdrom"]
